{
  "name": "dsnet",
  "version": "1.0.0",
  "description": "Simple wrapper for DSNET API.",
  "main": "dist/index.js",
	"module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
		"dist/index.js",
		"dist/index.esm.js",
    "dist/index.d.ts",
    "dist/types.d.ts",
    "dist/types.js"
	],
  "scripts": {
    "dev": "ts-node src/index.ts",
    "esm": "tsc --module esnext && cpy dist/index.js dist --rename index.esm.js",
    "cjs": "tsc --module commonjs",
    "build": "npm run esm && npm run cjs",
    "docs": "typedoc --out docs src",
    "prebuild": "del-cli dist",
    "prepublishOnly": "npm run build"
  },
  "author": "Jakub Kopa≈Ñko <hi@kopanko.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "del-cli": "^3.0.1",
    "cpy-cli": "^3.1.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.14",
    "typescript": "^4.1.3"
  }
}
